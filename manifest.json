{
    "manifest_version": 3,
    "name": "简洁书签",
    "description": "让你的书签更加简洁",
    "version": "1.0.0",
    "action": {
        "default_popup": "public/popup.html"
    },
    "icons": {
        "16": "public/images/logo16.png",
        "32": "public/images/logo32.png",
        "48": "public/images/logo48.png",
        "128": "public/images/logo128.png"
    },
    "permissions": [
        "bookmarks"
    ]
}